@model EntityLayer.Concrete.Message;
@{
    ViewData["Title"] = "DetailsContact";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}

<h1>DetailsContact</h1>

<form class="form-group">
    <div class="form-control">
        <br />
        @Html.Label("ID")
        @Html.TextBoxFor(x => x.MessageID, new { @class = "form-control" })
        <br />
        @Html.Label("isim")
        @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
        <br />
        @Html.Label("Mail")
        @Html.TextBoxFor(x => x.Mail, new { @class = "form-control" })
        <br />
        @Html.Label("Acıklama")
        @Html.TextAreaFor(x => x.Content, 10, 3, new { @class = "form-control" })
        <a asp-action="Index" class="btn btn-outline-primary mr-2"> İletişim Sayfası </a>
    </div>
</form>